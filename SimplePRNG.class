public class SimplePRNG {
    private long seed;

    public SimplePRNG(long seed) {
        this.seed = seed;
    }

    private long next() {
        seed = seed * 6364136223846793005L + 1;
        return seed;
    }

    public int nextInt() {
        return (int)(next() >>> 33);
    }

    public int nextInt(int bound) {
        return Math.abs(nextInt()) % bound;
    }

    public double nextDouble() {
        return (next() >>> 11) * (1.0 / (1L << 53));
    }

    public static void main(String[] args) {
        SimplePRNG rng = new SimplePRNG(12345);

        for (int i = 0; i < 5; i++)
            System.out.println(rng.nextInt());

        for (int i = 0; i < 5; i++)
            System.out.println(rng.nextInt(100));

        for (int i = 0; i < 5; i++)
            System.out.println(rng.nextDouble());
    }
}
